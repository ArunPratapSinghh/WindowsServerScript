###Script for windows server showing windows Activate Error#########
#
$list = get-content -path ./License_server.txt

#license check
foreach($VM in $list)
{
  try{
      invoke-command -computername $VM -scriptblock {
        slmgr.vbs ckms
`       slmgr.vbs /ato
}catch{
  write-host "Server is not pinging $VM" -forgroundcolor Red

}
}


